decl

integer fact,orial;boolean n[233];

enddecl


integer main( ){ // Main definition should always begin like this

decl

integer n[100],i,rvalue, factorial;

enddecl

begin

if (n[1]==i) then

rvalue = 1;

else

rvalue = n[1] * factorial/n[1];

endif;

read (n[1]);

i = 1;

while ( i <= n[1]) do

write ( factorial+i);

i = i + 1;

endwhile;

return 1; // Any integer value may be returned

end

}
