decl

integer fact,orial;boolean n[233];

enddecl


integer main( ){ // Main definition should always begin like this

decl

integer q,n[100],i,rvalue, factorial;

enddecl

begin

if (n[1]==1) then

rvalue = 1;

else

rvalue = 2 + 1 - 2;

rvalue =  n[100] * factorial/i;

endif;

read (q);

t = 1;

while ( i <= n) do

write ( factorial+i);

i = i + 1 *i;

endwhile;

return 1; // Any integer value may be returned

end

}
