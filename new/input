decl

integer fact,orial;boolean n[233];

enddecl


integer main( ){ // Main definition should always begin like this

decl

integer n,i ;

enddecl

begin

if (n==1) then

rvalue = 1;

else

rvalue = n * factorial/n[1];

endif;

read (n);

i = 1;

while ( i <= n) do

write ( factorial+i);

i = i + 1;

endwhile;

return 1; // Any integer value may be returned

end

}